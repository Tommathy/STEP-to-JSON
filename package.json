{
    "name": "step-to-json",
    "version": "3.0.0",
    "description": "A simple command line tool that extracts the system structure of a STEP (ISO 10303-44) file and outputs it as .json",
    "main": "dist/parser.cjs.js",
    "module": "dist/parser.esm.js",
    "browser": "dist/parser.umd.js",
    "type": "module",
    "scripts": {
        "demo": "npm run debug",
        "debug": "node ./demo/main.js",
        "lint": "eslint src test",
        "fix": "npx prettier . --write",
        "test": "vitest run",
        "prebuild": "rimraf dist",
        "build": "rollup -c",
        "prepublishOnly": "npm run lint && npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aljoshakoecher/STEP-to-JSON.git"
    },
    "keywords": [
        "STEP",
        "JSON",
        "parser",
        "assembly",
        "system",
        "structure"
    ],
    "author": "Aljosha KÃ¶cher",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/aljoshakoecher/STEP-to-JSON/issues"
    },
    "homepage": "https://github.com/aljoshakoecher/STEP-to-JSON#readme",
    "dependencies": {
        "cli-progress": "^3.12.0",
        "colors": "^1.4.0",
        "rxjs": "^7.8.1",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.0",
        "@babel/core": "^7.23.3",
        "@babel/eslint-parser": "^7.23.3",
        "@babel/node": "^7.22.19",
        "@babel/preset-env": "^7.23.3",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "babel-loader": "^9.1.3",
        "eslint": "^7.5.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "prettier": "3.4.2",
        "rimraf": "^5.0.5",
        "rollup": "^4.5.0",
        "vitest": "^1.6.0"
    }
}
